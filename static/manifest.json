{
    "name": "Text Filter",
    "version": "0.1.2",
    "manifest_version": 2,
    "icons": {
        "128": "icon128.png"
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Log Viewer"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": ["content.css"],
            "js": ["content.js"]
        }
    ],
    "permissions": ["*://*/*", "<all_urls>", "storage"]
}
